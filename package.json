  "description": "Typescript bindings for GJS",
  "main": "index.js",
  "scripts": {
    "test": "tsc test.ts && node_modules/ava/cli.js test.js",
    "regress": "tsc main.ts && nodejs main.js -m Gtk-3.0 -m Soup-2.4 -m GtkSource-3.0 -m WebKit2-4.0 -m AppIndicator3-0.1 -m Notify-0.7 -o out && cd out && tsc sam.ts",
